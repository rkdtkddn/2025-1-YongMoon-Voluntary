        "import torch\n",
        "data = [[1,2],[3,4]]\n",
        "x_data=torch.tensor(data)\n",
        "print(x_data)\n",
        "tensor = x_data\n",
        "tensor[:,1] = 0\n",
        "print(tensor)\n",
        "y1=tensor @ tensor.T\n",
        "\n",
        "y2=tensor.matmul(tensor.T)\n",
        "\n",
        "print(y1)\n",
        "print(y2)"
      ],
